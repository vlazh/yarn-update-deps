{
  "name": "yarn-update-deps",
  "displayName": "Yarn update deps",
  "description": "Interactive update dependencies",
  "version": "1.3.1",
  "license": "MIT",
  "publisher": "vlazh",
  "author": "Vladimir Zhukov",
  "maintainers": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:yarn-update-deps.update-deps",
    "onCommand:yarn-update-deps.update-yarn",
    "onCommand:yarn-update-deps.dedupe",
    "onCommand:yarn-update-deps.migrate-version"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Update Deps",
        "category": "Yarn",
        "command": "yarn-update-deps.update-deps"
      },
      {
        "title": "Update Yarn",
        "category": "Yarn",
        "command": "yarn-update-deps.update-yarn"
      },
      {
        "title": "Dedupe",
        "category": "Yarn",
        "command": "yarn-update-deps.dedupe"
      },
      {
        "title": "Migrate Yarn Version to Berry",
        "category": "Yarn",
        "command": "yarn-update-deps.migrate-version"
      }
    ]
  },
  "icon": "yarn_icon.png",
  "homepage": "https://github.com/vlazh/yarn-update-deps",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/yarn-update-deps"
  },
  "keywords": [
    "yarn",
    "update",
    "upgrade",
    "deps",
    "dependencies",
    "dedupe"
  ],
  "scripts": {
    "clean": "rimraf out/*",
    "vscode:prepublish": "yarn run clean && yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "publish:major": "vsce publish major",
    "publish:minor": "vsce publish minor",
    "publish:patch": "vsce publish patch"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.35.0",
    "@js-toolkit/configs": "3.95.1",
    "@types/eslint": "9.6.1",
    "@types/node": "24.5.2",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "9.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  }
}
